{
  "name": "ai-code-fixer",
  "displayName": "AI Code Fixer",
  "description": "AI powered code auto fixer for python",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aiCodeFixer.debug"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiCodeFixer.debug",
        "title": "Debug with AI Code Fixer"
      }
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.10",
    "eslint": "^9.23.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@monaco-editor/react": "^4.7.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "globals": "^16.1.0",
    "html-webpack-plugin": "^5.6.3",
    "monaco-editor": "^0.52.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-toastify": "^11.0.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.98.0"
  }
}
